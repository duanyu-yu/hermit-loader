/dts-v1/;

/ {
    cpus {
        #address-cells = <1>;
        #size-cells = <0>;
        cpu@0 {
            compatible = "arm,cortex-a9";
            reg = <0x0>; 
        };
        cpu@1 {
            compatible = "arm,cortex-a9";
            reg = <0x1>; 
        };
    };

    memory {
        device_type = "memory";
    };

    virtio_net@c0000000 {
        compatible = "virtio,mmio";
        reg = <0xc0000000 0x200>;
    };
};